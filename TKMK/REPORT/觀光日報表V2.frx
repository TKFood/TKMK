      <TableDataSource Name="Table2" DataType="System.Int32" Enabled="true" SelectCommand="SELECT SUM(TA026)  AS '10670205-SUMTA026'&#13;&#10;FROM [TK].dbo.POSTA TA1 &#13;&#10;WHERE TA1.TA001 &gt;= CONVERT(varchar(8), DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0), 112) &#13;&#10;AND TA1.TA002 IN ('106702','106705')&#13;&#10;AND TA1.TA001&lt;='20250422'&#13;&#10;">
        <Column Name="10670205-SUMTA026" DataType="System.Decimal"/>
